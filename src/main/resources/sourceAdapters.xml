<?xml version="1.0" encoding="UTF-8"?>
<adapters>

    <adapter name="Twitter" limit="true" siceDate="false" untilDate="true" languages="true" imdbID="false" cleanTweet="true">
        <class>es.uned.adapters.sources.TwitterSearch</class>
        <languages>
            <lang desc="Español">es</lang>
            <lang desc="Inglés">en</lang>
        </languages>
        <parameter name="De usuario" type="Text" id="usuarioID" default="@usuario">
            <description>Descripción del parámetro</description>
        </parameter>
    </adapter>

    <adapter name="Frase" limit="false" sinceDate="false" untilDate="false" languages="true" imdbID="false" cleanTweet="false">
        <class>es.uned.adapters.sources.SentenceSearch</class>
        <languages>
            <lang desc="Español">es</lang>
            <lang desc="Inglés">en</lang>
        </languages>
    </adapter>

    <adapter name="IMDB" limit="true" sinceDate="false" untilDate="false" languages="true" imdbID="true" cleanTweet="false">
        <class>es.uned.adapters.sources.IMDBSearch</class>
        <languages>
            <lang desc="Español">es</lang>
            <lang desc="Inglés">en</lang>
        </languages>
    </adapter>

    <adapter name="Trakt" limit="true" sinceDate="false" untilDate="false" languages="false" imdbID="true" cleanTweet="false">
        <class>es.uned.adapters.sources.TraktSearch</class>
        <languages>
            <lang desc="Inglés">en</lang>
        </languages>
    </adapter>

</adapters>